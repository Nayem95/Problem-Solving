#include<stdio.h>
int main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>0 && b>0)
        printf("1st quardant\n");
     if(a>0 && b<0)
        printf("2nd quardant\n");
     if(a<0 && b>0)
        printf("3rd quardant\n");
     if(a<0 && b<0)
        printf("4th quardant\n");

    return 0;
}
